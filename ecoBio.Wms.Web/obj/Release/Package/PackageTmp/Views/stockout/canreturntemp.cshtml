@{
    var list = Model.list as List<Enterprise.Invoicing.ViewModel.StockReturnDetailModel>;
    }
@if (list!=null&&list.Count>0)
{ 
<div class="right_area_active_list">
<table width="100%" border="0" cellspacing="0" cellpadding="0">
      <thead>
  <tr>
    <th width="5%" align="center">&nbsp;</th>
 <th width="16%" height="20" align="center">出库单号</th>
    <th width="15%">物料名称</th>
    <th width="15%">物料规格</th>
    <th width="5%">单位</th>
    <th width="10%">领料仓库</th>
    <th width="8%">领料数量</th>
    <th width="10%">退回仓库</th>
    <th width="8%" >退回数量</th>
    <th width="8%" >备注</th>
  </tr>
  </thead>
    <tbody>
    @foreach (var item in list)
    {
        if (item.type == "delete")
        {
            continue; 
        }
          <tr>
              <td>
                  <span class="round_delete_icon" onclick="deldetailtemp('@item.stockoutDetailSn','@item.materialModel')" style="margin-right:10px;"></span>                
              </td>
   <td>@item.stockoutNo</td>
    <td>@item.materialName</td>
    <td>@item.materialModel</td>
    <td>@item.materialUnit</td>
      <td>@item.fromdepotName</td>
      <td>@item.outAmoutn</td>
      <td>@item.todepotName</td>
      <td>@item.returnAmount</td><td>@item.remark</td>
  </tr>
    }
        </tbody>
</table>
</div>

    <div style=" text-align:center; height:46px; padding-top:10px; width:765px; background:#f7fbff; border-top:1px solid #c6d9e7;">
<a class="btn_refer2" href="javascript:void(0);" onclick="savestockin()">提交</a>
</div>
}