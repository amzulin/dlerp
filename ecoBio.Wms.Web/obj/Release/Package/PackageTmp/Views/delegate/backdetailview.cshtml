
@{
    var no = Model.no;
    var list = Model.list as List<Enterprise.Invoicing.Entities.Models.V_DelegateBackDetail>;

    var pi = 1;
    }
<table  width="94%" border="0" cellspacing="0" cellpadding="0" class="table_common_yellow ">
      <thead>
        <tr>
          <th width="5%">明细</th>
          <th width="6%">类别</th>
          <th width="14%">委外单号</th>
          <th width="10%">物料编码</th>
          <th width="18%">物料名称</th>
          <th width="18%">物料规格</th>
          <th width="6%" >单位</th>
          <th width="8%">数量</th>
          <th width="15%">备注</th>
        </tr>
      </thead>
          <tbody>
     @if (list.Count>0)
     {
        foreach (var item in list)
     {  
  <tr>
      <td>@pi</td>
    <td>@(item.isProduct?"产品":"原料")</td>
    <td>@item.fromDelegateNo</td>
    <td>@item.materialNo</td>
    <td>@item.materialName</td>
    <td>@item.materialModel</td>
    <td>@item.unit</td>
    <td>@item.backAmount</td>
    <td>@item.remark</td>
  </tr>
         pi++;
      }    
     }
     else
     {
        <tr>
  <td colspan="9"  bgcolor="#FFFFFF" style="text-align:left;">无交货明细</td></tr> 
     }
</tbody>
    </table> 

