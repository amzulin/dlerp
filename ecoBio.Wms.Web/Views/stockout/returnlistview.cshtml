
@{
    var no = Model.no;
    //var one = Model.one as Enterprise.Invoicing.ViewModel.ReturnModel;
    var list = Model.list as List<Enterprise.Invoicing.ViewModel.StockReturnDetailModel>;
    //var mc = Model.mc as List<Enterprise.Invoicing.ViewModel.KeyValue>;
    var pi = 1;
    }
<table  width="94%" border="0" cellspacing="0" cellpadding="0" class="table_common_yellow ">
      <thead>
        <tr>
          <th width="5%" align="center">明细</th>
    <th width="14%" height="20" align="center">出库单号</th>
    <th width="15%">物料名称</th>
    <th width="15%">物料规格</th>
    <th width="5%">单位</th>
    <th width="10%">领料仓库</th>
    <th width="8%">领料数量</th>
    <th width="10%">退回仓库</th>
    <th width="8%" >退回数量</th>
    <th width="10%" >备注</th>
        </tr>
      </thead>
          <tbody>
     @if (list.Count>0)
     {
        foreach (var item in list)
     {  
  <tr>
      <td>@pi</td>
      <td><a href="../stockout/outlist?no=@item.stockoutNo"> @item.stockoutNo</a></td>
    <td title="编码 @item.materialNo">@item.materialName</td>
    <td title="图号 @item.materialTu">@item.materialModel</td>
    <td>@item.materialUnit</td>
      <td>@item.fromdepotName</td>
      <td>@item.outAmoutn</td>
      <td>@item.todepotName</td>
      <td>@item.returnAmount</td><td>@item.remark</td>
  </tr>
         pi++;
      }    
     }
     else
     {
        <tr>
  <td colspan="10"  bgcolor="#FFFFFF" style="text-align:left;">无退单明细</td></tr> 
     }
</tbody>
    </table> 

